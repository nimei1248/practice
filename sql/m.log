## mongodb process
port=$(grep 'port=' $(ps -ef | grep 'bin/mongod' | grep -v 'grep' | awk '{print $10}') | awk -F '=' '{print $2}')
inst=$(ps -ef | grep 'bin/mongod' | grep -v 'grep' | awk '{print $10}' | awk -F '/' '{print $4}')
echo -e "${GREEN_COLOR}$port" "$inst" "$(ps -ef | grep 'bin/mongod' | grep -v 'grep' | awk '{print $8,$10}' | sort)${RES}"
#alias mylist="ps -ef | grep 'bin/mongod' | grep -v 'grep' | awk '{print \$8,\$10}' | sort"
alias mylist="echo -e '${GREEN_COLOR}'$port $inst $(ps -ef | grep 'bin/mongod' | grep -v 'grep' | awk '{print $8,$10}' | sort)'${RES}'"


## grep alias
alias grep='grep --color'
alias egrep='egrep --color'


IP=$(/sbin/ip route get 1 | awk '{print $NF;exit}')
alias colormysql=$(echo -e '/usr/local/mysql_5.7.18-15/bin/mysql --prompt="[\x1B[1;31m\\c \x1B[1;32m\\u\x1B[1;33m@\x1B[1;35m\\h\e[1;36m $IP:\x1B[1;32m\\p \x1B[1;35m$(date +%F)_\R:\m:\s_$(date +%u_%Z) \x1B[1;36m(\\d)\x1B[0m]\nSQL> "')

alias inst1_startup="${MONGODB_HOME}/bin/mongod -f /data/mongodb/inst1/mongodb.cnf"
