$ cat .mongorc.js   
cmdCount = 1;
hosts = db.serverStatus().host;

prompt = function() {
             return "[" + (cmdCount++) + ' ' + "uptime:" + db.serverStatus().uptime + ' ' + db + "@" + hosts + ' ' + "conns:" + db.serverStatus().connections.current + ' ' + "documents:" + db.stats().objects + "]" + '\n' + "mongodb> "
         }



## color
GREEN_COLOR='\E[1;32m'
RED_COLOR='\E[1;31m'
RES='\E[0m'


### process
#echo -e "${GREEN_COLOR}$(ps -ef | grep 'bin/mysqld' | grep 'socket' | awk -F '[ = ]*' '{print $24"  "$8,$10,$22,$14,$3,$2}' | sort)${RES}"
